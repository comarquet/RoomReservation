<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Ignore EI issues in entire codebase -->
    <Match>
        <Bug pattern="EI,EI2"/>
    </Match>
    <!-- Ignore SS issues in tests -->
    <Match>
        <Bug pattern="SS"/>
        <Class name="~.*Test"/>
    </Match>
</FindBugsFilter>